{
    "manifest_version": 2,
    "name": "Erik Launcher",
    "description": "Quick Launch Erik",
    "version": "1.0.0",
    "icons": {"128": "images/icon.png"},
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}